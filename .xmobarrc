Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
        , borderColor = "black"
        , border = TopB
        , bgColor = "black"
        , fgColor = "grey"
        , position = TopW L 100
        , commands = [ Run Network "eth0" ["-w","4","-L","0","-H","32","--normal","green","--high","red"] 10
                        , Run Network "wlan0" ["-w","4","-L","0","-H","32","--normal","green","--high","red"] 10
                        , Run Com "wifi_strength" [] "wifi" 30
                        , Run Cpu ["-w","2","-L","3","-H","50","--normal","green","--high","red"] 10
                        , Run Memory ["-w","2","-t","Mem: <usedratio>%"] 10
                        , Run Swap ["-w","2"] 10
                        , Run Date "%Y/%m/%d %H:%M:%S" "date" 10
                        ]
        , sepChar = "%"
        , alignSep = "}{"
        , template = " %cpu% | %memory% * %swap% | %wifi% }{ %eth0% %wlan0%      <fc=#ee9a00>%date%</fc>"
        }
